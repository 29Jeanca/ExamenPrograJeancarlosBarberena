@model IEnumerable<Profesores>
@{
    ViewData["Buscar"] = "Resultados de búsqueda";
}

<div class="list-group">
  @foreach (var item in Model)
  {
      <a  asp-action="VerProfesor" asp-route-id="@item.ID"  
      class="list-group-item list-group-item-action">@item.nombreCompletoProfesor</a>      
  }
  </div>
